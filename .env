# General application configuration
APP_ENV=development
COOKIE_SECURE=false
JWT_SECRET=supersecretjwtkey
SESSION_TIMEOUT_MS=600000
LOG_LEVEL=debug # debug, info, warning, error
LOG_FILE_PATH=/var/www/html/logs

# Database configuration (all values are required)
DB_HOST=db
DB_PORT=3306
DB_DATABASE=finhub
DB_USERNAME=finhub
DB_PASSWORD=finhub
